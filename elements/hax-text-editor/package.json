{
  "name": "@lrnwebcomponents/hax-text-editor",
  "wcfactory": {
    "className": "HaxTextEditor",
    "customElementClass": "LitElement",
    "elementName": "hax-text-editor",
    "generator-wcfactory-version": "0.8.7",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/hax-text-editor.css",
      "scss": "src/hax-text-editor.scss",
      "html": "src/hax-text-editor.html",
      "js": "src/hax-text-editor.js",
      "properties": "src/hax-text-editor-properties.json",
      "hax": "src/hax-text-editor-hax.json"
    },
    "sharedHaxProps": [],
    "sharedProps": [],
    "sharedStyles": []
  },
  "version": "3.0.6",
  "description": "text editor, toolbar, and toolbar buttons for HAX",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "hax-text-editor.js",
  "module": "hax-text-editor.js",
  "umd": "hax-text-editor.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/hax-text-editor/test/",
    "start": "yarn run dev",
    "build": "gulp --gulpfile=gulpfile.cjs && rollup -c && prettier --ignore-path ../../.prettierignore --write \"**/*.{js,json}\" && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "gulp dev --gulpfile=gulpfile.cjs",
    "serve": "es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "gulp lighthouse --gulpfile=gulpfile.cjs"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lit-element":"2.0.1",
    "@lrnwebcomponents/lrn-math": "^3.0.6",
    "@lrnwebcomponents/lrn-vocab": "^3.0.6",
    "@lrnwebcomponents/oer-schema": "^3.0.6",
    "@lrnwebcomponents/hax-iconset": "^3.0.6",
    "@lrnwebcomponents/rich-text-editor": "^3.0.6"
  },
  "devDependencies": {
    "web-animations-js":"2.3.1","@lrnwebcomponents/deduping-fix":"2.0.5","@polymer/iron-demo-helpers":"3.1.0","@polymer/iron-component-page":"github:PolymerElements/iron-component-page","@webcomponents/webcomponentsjs":"2.2.7","concurrently":"4.1.0","gulp-babel":"8.0.0","gulp-sourcemaps":"2.6.5","lodash":"4.17.11","polymer-cli":"1.9.7","polymer-build":"3.1.2","wct-browser-legacy":"1.0.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
